// ProductoNnumeros(Practico3).cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int serie(int n);

void main()
{
	int n, p=1;
	cout<<"Ingrese el limite de la serie"<<endl;
	cin>>n;

	p=serie(n);
	cout<<"El resultado de la serie es: "<<p<<endl;
	getch ();
}

int serie(int n)
{
	int p=1;
	for(int i=1; i<=n; i++){
		p=p*i;
	}
	return p;
}
